addpath(genpath('attacks'));

imagesDir = "images/";

images = dir(imagesDir);
images = {images.name};
images = images(3:end);
images = strcat(imagesDir, images);
images = images';
[x,y] = size(images);
results = zeros(x, 2);
rng(2);
tmpW=round(rand(1,1024));
save('tmpWatermark','tmpW');

for i = 6:6%size(images)
    original    = imread(images(i));

    watermarked = embedding(images(i));
    attacked    = test_awgn(watermarked, 0.00000000001, now);
    [results(i, 1), results(i, 2)]  = detection(original, watermarked, attacked);
end

results